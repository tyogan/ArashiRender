#version 330 core

in vec3 fWorldPos;
in vec3 fWorldNormal;
in vec2 fTexCoords;

out vec4 FragColor;

uniform vec3 objectColor;

uniform samplerCube irradianceMap;
uniform sampler2D ssaoMap;

void main()
{	
	vec3 l;
	vec3 n=normalize(fWorldNormal);
	vec3 env=vec3(0.f);
	
	vec3 res=env*objectColor;
	FragColor=vec4(pow(res,vec3(1/2.2)),1);
}